{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "background": "#FFFAE5",
    "width": 1000,
    "height": 400,
    "data": {
      "url": "https://raw.githubusercontent.com/AdrianEngCheeErn/Assignment2/refs/heads/main/CPI%2C%20Goods%20and%20Services%20components%2C%20annual%20movement%20(%25).csv"
    },
    "encoding": {
      "x": {
        "field": "Date",
        "type": "temporal",
        "title": "Date"
      }
    },
    "layer": [
      {
        "mark": {
          "type": "line",
          "color": "#FF6347"
        },
        "encoding": {
          "y": {
            "field": "Goods (%)",
            "type": "quantitative",
            "title": "Percentage (%)"
          },
          "tooltip": [
            { "field": "Date", "type": "temporal" },
            { "field": "Goods (%)", "type": "quantitative" }
          ]
        }
      },
      {
        "mark": {
          "type": "line",
          "color": "#1E90FF"
        },
        "encoding": {
          "y": {
            "field": "Services (%)",
            "type": "quantitative",
            "title": "Percentage (%)"
          },
          "tooltip": [
            { "field": "Date", "type": "temporal" },
            { "field": "Services (%)", "type": "quantitative" }
          ]
        }
      }
    ]
  }